{
  "name": "@robgietema/nick",
  "description": "Isometric Backend",
  "maintainers": [
    {
      "name": "Rob Gietema",
      "email": "rob.gietema@gmail.com",
      "url": "https://robgietema.nl"
    }
  ],
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:robgietema/nick.git"
  },
  "bugs": {
    "url": "https://github.com/robgietema/nick/issues",
    "email": "plone-developers@lists.sourceforge.net"
  },
  "homepage": "https://plone.org",
  "keywords": [
    "react"
  ],
  "scripts": {
    "start": "nodemon --exec babel-node src/server.js",
    "extract": "nodemon --exec babel-node scripts/extract.js",
    "test": "jest --runInBand",
    "coverage": "jest --coverage --runInBand",
    "knex": "babel-node --plugins ./src/plugins/strip-i18n.js node_modules/.bin/knex",
    "i18n": "babel-node scripts/i18n.js",
    "migrate": "yarn knex migrate:latest",
    "seed": "yarn knex seed:run",
    "rollback": "yarn knex migrate:rollback --all",
    "reset": "yarn knex migrate:rollback --all && yarn knex migrate:latest && yarn knex seed:run"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "engines": {
    "node": "~16.13.0"
  },
  "dependencies": {
    "@babel/core": "7.17.5",
    "@babel/node": "7.16.8",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@formatjs/cli": "4.8.2",
    "@formatjs/intl": "2.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "bcrypt": "5.0.1",
    "bcrypt-promise": "2.0.0",
    "body-parser": "1.19.2",
    "deepmerge": "4.2.2",
    "express": "4.17.3",
    "jest": "27.5.1",
    "jsonwebtoken": "8.5.1",
    "knex": "1.0.3",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "nodemon": "2.0.15",
    "objection": "3.0.1",
    "objection-table-name": "2.0.2",
    "pg": "8.7.3",
    "prettier": "2.5.1",
    "sharp": "0.30.3",
    "slugify": "1.6.5",
    "supertest": "6.2.2",
    "supertest-as-promised": "4.0.2",
    "uuid": "8.3.2"
  }
}
